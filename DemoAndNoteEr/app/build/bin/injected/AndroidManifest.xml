<?xml version="1.0" encoding="utf-8"?>
<manifest
	xmlns:android="http://schemas.android.com/apk/res/android"
	package="demo.and.note.er"
	android:versionCode="1"
	android:versionName="1.0">

	<uses-sdk
		android:minSdkVersion="14"
		android:targetSdkVersion="21"/>

	<application
		android:allowBackup="true"
		android:icon="@drawable/ic_launcher"
		android:label="@string/app_name"
		android:theme="@style/AppTheme">

		<activity
			android:name=".ShowInstalledAc"
			android:label="ShowInstalledAc类的布局"
			android:launchMode="singleTask"/>

		<activity
			android:name=".WebViewDemoAc"
			android:label="WebViewDemoAc类的布局"
			android:launchMode="singleTask"/>

		<activity
			android:name=".ServerRequestAc"
			android:label="ServerRequestAc类的布局"
			android:launchMode="singleTask"/>

		<activity
			android:name=".NotificationContentAc"
			android:label="NotificationContentAc类的布局"
			android:launchMode="singleTask"/>

		<activity
			android:name=".ApkSelectErAc"
			android:label="ApkSelectErAc类的布局"
			android:launchMode="singleTask"/>

		<activity
			android:name=".YingYongBaoGuanLiQiAc"
			android:label="YingYongBaoGuanLiQi类的布局"
			android:launchMode="singleTask"/>

		<activity
			android:name=".SimpleXFCActivity"
			android:label="SimpleXFCActivity类的布局"
			android:launchMode="singleTask"/>

		<activity
			android:name=".DaKaiYanShiBiJiAc"
			android:label="DaKaiYanShiBiJi类的布局"
			android:launchMode="singleTask"/>

		<activity
			android:name=".MainActivity"
			android:label="@string/app_name"
			android:launchMode="singleTask">

			<intent-filter>

				<action
					android:name="android.intent.action.MAIN"/>

				<category
					android:name="android.intent.category.LAUNCHER"/>

			</intent-filter>

		</activity>

		<service
			android:name=".FuZhuInstaller"
			android:label="DemoAndNoteEr辅助安装服务"
			android:permission="android.permission.BIND_ACCESSIBILITY_SERVICE">

			<intent-filter>

				<action
					android:name="android.accessibilityservice.AccessibilityService"/>

			</intent-filter>

			<meta-data
				android:name="android.accessibilityservice"
				android:resource="@xml/fu_zhu_installer_description"/>

		</service>

		<service
			android:name=".XFCService"
			android:label="悬浮窗服务"/>

	</application>

	<uses-permission
		android:name="android.permission.SYSTEM_ALERT_WINDOW"/>

	<uses-permission
		android:name="android.permission.READ_EXTERNAL_STORAGE"/>

	<uses-permission
		android:name="android.permission.INTERNET"/>

</manifest>

